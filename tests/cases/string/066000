function main():
	tmp = (
		('\u0000',1),
		('\u2010',1),
		('\U00000000',1),
		('\U00002010',1),
		('\u0000a',2),
		('a\u2010',2),
		('\U00000000b',2),
		('b\U00002010',2),
		('\r\r',2),
		('\r\n',2),
		('\n\r',2),
		('\n\n',2),
		)
	cnt : for a,b = tmp:
		if a.size() != b:
			return cnt
	return 'ok'
+++value 'ok'
