function main():
	a='a'
	b='b'
	class T1:
		function $withenter():
			return self
		function $withexit():
			a:='o'
			return 'qwe'
	class T2:
		function $init(): raise $exception()
		function $withenter():
			return self
		function $withexit():
			b:='k'
			return 'qwe'
	try:
		with T1(),T2():
			return 'abc'
	except:
		return a+b
	return 'a'
+++value 'ob'
