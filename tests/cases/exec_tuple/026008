class T:
	function $hash(): raise $exception()
function main():
	z=$tuple.$rmul
	z=z.bind(('a','b','c'))
	if z(2)!=('a','b','c','a','b','c'): return 5
	z=$tuple.$hash
	z=z.bind($tuple([]))
	if $hash(())!=z(): return 1
	if $str($tuple())!='()': return 2
	a=('a','b','c','d')
	if a[3:-2]!=(): return 3
	if a[1:3]!=('b','c'): return 4
	z=('a','b')
	if z*3!=('a','b','a','b','a','b'): return 6
	$hash(('a','b',T(),'d'))
+++exception YEXCEPTION_USER 2 0 0 16
